        "LVL_TUTORIAL": {
            "AREA_TEXT_0": {
                "MAIN": {
                    "EN": "can you hear me?",
                    "RO": "mă auzi?",
					"RU": "ты меня слышишь?"
                },
                "SUB": {
                    "EN": "Use WASD to move around.",
                    "RO": "Folosește WASD pentru a te mișca.",
					"RU": "Используй WASD, чтобы передвигаться."
